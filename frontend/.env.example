# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_WS_URL=ws://localhost:5000

# Authentication
REACT_APP_JWT_SECRET=your_jwt_secret_here

# Environment
NODE_ENV=development

